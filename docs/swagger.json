    "host": "localhost:8080",
    "basePath": "/v1",
    "paths": {
        "/fibonacci/current": {
            "get": {
                "description": "get the current fibonacci number in the sequence",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Current Fibonacci number",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "/fibonacci/next": {
            "post": {
                "description": "get the next fibonacci number in the sequence",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Next Fibonacci number",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "/fibonacci/previous": {
            "post": {
                "description": "get the previous fibonacci number in the sequence",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Previous Fibonacci number",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "integer"
                        }
                    }
                }
            }
        }
    }
}